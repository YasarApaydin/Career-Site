@model IsVerenViewModel
    <!-- Page Title Start -->
    <section class="page-title title-bg13">
        <div class="d-table">
            <div class="d-table-cell">
                <h2>Sign Up</h2>
                <ul>
                    <li>
                        <a href="index.html">Home</a>
                    </li>
                    <li>Sign Up</li>
                </ul>
            </div>
        </div>
        <div class="lines">
            <div class="line"></div>
            <div class="line"></div>
            <div class="line"></div>
        </div>
    </section>
    <!-- Page Title End -->
    <!-- Sign up Section Start -->


<form  enctype="multipart/form-data" method="post">
    @Html.AntiForgeryToken()
    @Html.HiddenFor(x => x.KullaniciTur)
    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
    <div class="signup-section ptb-100">
        <div class="container">
            <div class="row">
                <div class="col-lg-6 col-md-8 offset-md-2 offset-lg-3">

                    @await Html.PartialAsync("UyeBilgilerFormu", Model)
                </div>
            </div>
        </div>
    </div>
    <!-- Sign Up Section End -->
    <div class="signup-section ptb-10">
        <div class="container">
            <div class="row">
                <div class="col-lg-6 col-md-8 offset-md-2 offset-lg-3">
                    @await Html.PartialAsync("KisiselBilgilerFormu", Model)
                </div>
            </div>
        </div>
    </div>
</form>


   